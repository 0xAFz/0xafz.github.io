{
  "name": "mdx-blog",
  "version": "1.0.1",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "server": "yarn build && NODE_ENV=production node server.js",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "export": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll",
    "deploy": "gh-pages --dotfiles=true --dist out/"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.5.1",
    "@next/mdx": "^9.1.1",
    "next": "latest",
    "parse-numeric-range": "^1.2.0",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.11.0",
    "styled-components": "^5.2.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.11.10",
    "@types/parse-numeric-range": "^0.0.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "6.1.5",
    "@types/styled-components": "^5.1.4",
    "babel-plugin-import-glob-array": "^0.2.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-styled-components": "^1.11.1",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "gray-matter": "^4.0.2",
    "mem": "^8.0.0",
    "next-offline": "^5.0.3",
    "prettier": "^2.1.2",
    "stringify-object": "^3.3.0",
    "terser": "^4.6.13",
    "typescript": "^4.0.3"
  }
}